r-pointer hover:bg-gray-600 bg-gray-400 text-white w-full\" value=\"Register\">\n      </div>\n    </form>\n</template>\n\n<script>\nimport axios from '../config/axios'\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n      name: ''\n    }\n  }, \n  methods: {\n    goLogin () {\n      this.$emit('goLogin')\n    },\n    register () {\n      const name = this.name\n      const email = this.email\n      const password = this.password\n\n      axios({\n        method: 'POST',\n        url: '/users/register',\n        data: {\n          name, email, password\n        }\n      })\n      .then(({ data }) =>{\n        this.$noty.success(data.message)\n        this.goLogin()\n        \n      })\n      .catch(({ response }) => {\n        this.$noty.error(response.data)\n        if(response.data.errors) this.$noty.error(response.data.errors.join('/n'))\n      })\n    }  \n  }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"4960fbacea207579d95c442660d12271","cacheData":{"env":{}}}