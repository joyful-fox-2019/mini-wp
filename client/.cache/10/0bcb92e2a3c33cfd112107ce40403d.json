m('token')\n        }\n      })\n      .then(({ data }) => {\n        this.articles = data\n      })\n      .catch(({ response }) => {\n        // error alert\n        console.log(response.data)\n      })\n    },\n    showArticle (id) {\n      this.$router.push(`articles/${id}`)\n    }\n  }, \n  created () {\n    if(!this.keyword) {\n      this.getArticles()\n    } else {\n      // console.log(this.keyword, 'article list');\n      this.getArticles(this.keyword)\n    }\n  },\n  watch: {\n    keyword () {\n      this.getArticles(this.keyword)\n    }\n  },\n}\n</script>\n\n<style>\n h2 {\n   font-size: 15px;\n }\n</style>"},"lineCount":null}},"error":null,"hash":"76316e73d3ad9ec8be898392759c7dcb","cacheData":{"env":{}}}