emplate>\n\n<script>\nimport axios from '../config/axios'\nexport default {\n  data() {\n    return {\n      email: '',\n      password: ''\n    }\n  },\n  methods: {\n    goRegister() {\n      this.$emit('goRegister')\n    },\n    login () {\n      const email = this.email\n      const password = this.password\n      \n      axios({\n        method: 'POST',\n        url: `/users/login`,\n        data: { email, password }\n      })\n      .then(({ data }) => {\n        this.$noty.success(data.message)\n        localStorage.setItem('token', data.token)\n          this.$emit('loggedIn')\n        console.log(data.message);\n      })\n      .catch(({ response }) => {\n        this.$noty.error(response.data.message)\n      })\n    }\n  },\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"c0525305a620a3d38d56a03a1dce0161","cacheData":{"env":{}}}