outer.push(`/edit/${id}`)\n    }\n  },\n  created () {\n    const id = this.$route.params.id\n    axios({\n      method: 'GET',\n      url: `/articles/${id}?mode=read`,\n      headers: {\n        token: localStorage.getItem('token')\n      }\n    })\n    .then(({ data }) => {\n      console.log(data);\n      this.article = data\n    })\n    .catch(({ response }) => {\n      console.log(response.data)\n    })\n  },\n}\n</script>\n\n<style scoped>\n  h1 {\n    font-size: 25px\n  }\n  .main-content {\n    overflow-y: auto;\n    overflow-x: hidden;\n  }\n</style>"},"lineCount":null}},"error":null,"hash":"f0ea91d625f21919902edddfd27f712a","cacheData":{"env":{}}}