ame":null,"original":{"line":194,"column":0},"generated":{"line":204,"column":2}},{"source":"components/navBar.vue","name":null,"original":{"line":194,"column":0},"generated":{"line":204,"column":12}},{"source":"components/navBar.vue","name":null,"original":{"line":194,"column":0},"generated":{"line":204,"column":14}},{"source":"components/navBar.vue","name":null,"original":{"line":195,"column":0},"generated":{"line":205,"column":0}},{"source":"components/navBar.vue","name":null,"original":{"line":195,"column":0},"generated":{"line":205,"column":4}},{"source":"components/navBar.vue","name":null,"original":{"line":195,"column":0},"generated":{"line":205,"column":15}},{"source":"components/navBar.vue","name":null,"original":{"line":195,"column":0},"generated":{"line":205,"column":17}},{"source":"components/navBar.vue","name":null,"original":{"line":194,"column":0},"generated":{"line":206,"column":0}},{"source":"components/navBar.vue","name":null,"original":{"line":149,"column":0},"generated":{"line":207,"column":0}}],"sources":{"components/navBar.vue":"<template>\n  <div>\n    <nav class=\"navbar navbar-expand-md navbar-dark fixed-top\" style=\"background-color: #145474;\">\n      <button class=\"navbar-brand btn\" onclick=\"home();\">\n        <img src=\"../img/wordpress.png\" alt=\"chatIcon\" width=\"40\" height=\"35\" class=\"mr-2 button\" />\n        My Sites\n      </button>\n      <button\n        class=\"navbar-toggler\"\n        type=\"button\"\n        data-toggle=\"collapse\"\n        data-target=\"#navbarsExampleDefault\"\n        aria-controls=\"navbarsExampleDefault\"\n        aria-expanded=\"false\"\n        aria-label=\"Toggle navigation\"\n      >\n        <span class=\"navbar-toggler-icon\"></span>\n      </button>\n\n      <div class=\"collapse navbar-collapse\" id=\"navbarsExampleDefault\">\n        <ul class=\"navbar-nav mr-auto\">\n          <li class=\"nav-item active text-white\">\n            <!-- <a class=\"nav-link btn\" href=\"#\">\n              <img\n                src=\"../img/newspaper.png\"\n                alt=\"chatIcon\"\n                width=\"28\"\n                height=\"25\"\n                class=\"mr-2 button\"\n              />\n              Reader\n              <span class=\"sr-only\">(current)</span>\n            </a>-->\n            <h3>User {{usernameLogin}}</h3>\n          </li>\n        </ul>\n\n        <button\n          class=\"btn btn-light mr-5\"\n          style=\" color:#145474; width: 100px;\"\n          data-toggle=\"modal\"\n          data-target=\"#writeModalArticle\"\n        >\n          <i class=\"far fa-edit\"></i>\n          Write\n        </button>\n\n        <a href=\"#\" class=\"btn\">\n          <img\n            src=\"../img/whiteChatIcon.png\"\n            alt=\"chatIcon\"\n            width=\"25\"\n            height=\"25\"\n            class=\"mr-2 button\"\n          />\n        </a>\n      </div>\n    </nav>\n\n    <div\n      class=\"modal fade\"\n      id=\"writeModalArticle\"\n      tabindex=\"-1\"\n      role=\"dialog\"\n      aria-labelledby=\"exampleModalLabel\"\n      aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"exampleModalLabel\">Write Article</h5>\n            <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n              <span aria-hidden=\"true\">&times;</span>\n            </button>\n          </div>\n          <div class=\"modal-body\" style=\"height: 900px\">\n            <div class style=\"width: 100%;\">\n              <!-- <div class=\"\" style=\"height: 350px;\" id=\"editor\">\n                                <h1>Edit Area</h1>\n                                <p>Hello World!</p>\n                                <p>Some initial <strong>bold</strong> text</p>\n                                <p><br></p>\n              </div>-->\n\n              <form>\n                <div class=\"form-group\">\n                  <label for=\"exampleInputTitle\">title</label>\n                  <input\n                    type=\"text\"\n                    class=\"form-control\"\n                    aria-describedby=\"emailHelp\"\n                    v-model=\"inputTitle\"\n                    placeholder=\"Enter Title\"\n                  />\n                </div>\n                <div class=\"form-group\">\n                  <label for=\"exampleFormControlTextarea1\">Content</label>\n                  <!-- <textarea\n                    class=\"form-control\"\n                    id=\"exampleFormControlTextarea1\"\n                    rows=\"3\"\n                    v-model=\"inputContent\"\n                  ></textarea>-->\n                </div>\n                <quillEditor v-model=\"inputContent\" style=\"height: 370px\"></quillEditor>\n\n                <div class=\"form-group\" style=\"margin-top: 120px\">\n                  <label for=\"exampleFormControlFile1\">input img</label>\n                  <input\n                    type=\"file\"\n                    v-on:change=\"handleFileEditor\"\n                    ref=\"file\"\n                    accept=\"image/*\"\n                    class=\"form-control-file\"\n                    id=\"exampleFormControlFile1\"\n                  />\n                </div>\n                <button\n                  type=\"button\"\n                  class=\"btn btn-primary\"\n                  data-dismiss=\"modal\"\n                  v-on:click.prevent=\"addArticle\"\n                  style=\"margin-top: 50px\"\n                >\n                  add\n                  Article\n                </button>\n              </form>\n            </div>\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close</button>\n            <!-- <button type=\"button\" class=\"btn btn-primary\">Save changes</button> -->\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport \"quill/dist/quill.core.css\";\nimport \"quill/dist/quill.snow.css\";\nimport \"quill/dist/quill.bubble.css\";\n\nimport { quillEditor } from \"vue-quill-editor\";\n\nexport default {\n  data() {\n    return {\n      inputTitle: \"\",\n      inputContent: \"\",\n      inputUsername: \"\",\n      imgFile: \"\",\n      usernameLogin: localStorage.getItem(\"username\")\n    };\n  },\n  methods: {\n    handleFileEditor() {\n      this.imgFile = event.target.files[0];\n    },\n    addArticle() {\n      let formData = new FormData();\n\n      formData.append(\"img\", this.imgFile);\n      formData.append(\"title\", this.inputTitle);\n      formData.append(\"content\", this.inputContent);\n\n      console.log(formData);\n      axios({\n        method: \"post\",\n        url: \"http://localhost:3000/article/\",\n        data: formData,\n        headers: {\n          token: localStorage.getItem(\"token\")\n        }\n      })\n        .then(response => {\n          Swal.fire({\n            title: \"Success\",\n            text: \"add article Success\",\n            icon: \"success\"\n          });\n          this.inputTitle = \"\";\n          this.inputContent = \"\";\n          this.$emit(\"addEvent\");\n        })\n        .catch(err => {\n          console.log(err);\n        });\n    }\n  },\n  components: {\n    quillEditor\n  }\n};\n</script>\n\n<style>\n</style>"},"lineCount":null}},"error":null,"hash":"76eb3c4e94afbbab6d9cf61f18fc229b","cacheData":{"env":{}}}