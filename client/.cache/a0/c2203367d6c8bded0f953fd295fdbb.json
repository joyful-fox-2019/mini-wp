}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":85,"column":0},"generated":{"line":138,"column":9}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":128,"column":0},"generated":{"line":138,"column":30}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":129,"column":0},"generated":{"line":139,"column":0}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":129,"column":0},"generated":{"line":139,"column":9}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":129,"column":0},"generated":{"line":139,"column":22}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":130,"column":0},"generated":{"line":140,"column":0}},{"source":"src/components/PagePersonal.vue","name":null,"original":{"line":85,"column":0},"generated":{"line":141,"column":0}}],"sources":{"src/components/PagePersonal.vue":"<template>\n  <div>\n\n    <div class=\"container\" id=\"image-container\" >\n      <div class=\"container\" id=\"detail-image\" v-for=\"(article, i) in articles\" :key=\"i\">\n\n        <div class=\"card\">\n          <div class=\"card-image\">\n            <figure class=\"image is-4by3\">\n              <a @click=\"isCardModalActive = true; index = i\" href=\"#\">\n                <img\n                  :src=\"article.featured_image[0]\"\n                  alt=\"Placeholder image\"\n                />\n              </a>\n            </figure>\n            <div class=\"contents\">\n\n              <h2 style=\"font-weight:bold; font-size:22px\">{{article.title}}</h2>\n              <hr style=\"margin-top:10px; margin-bottom:10px\">\n            </div>\n          </div>\n        </div>\n\n      </div>\n    </div>\n\n    <b-modal :active.sync=\"isCardModalActive\" :width=\"1000\" scroll=\"keep\">\n      <div class=\"card\">\n        <div class=\"card-image\">\n          <figure class=\"image is-4by3\">\n              <img\n                :src=\"articles[index].featured_image[0]\"\n                alt=\"Image\"\n              />\n          </figure>\n          </div>\n          <div class=\"card-content\">\n            <div class=\"media\">\n              <div class=\"media-left\"></div>\n                <div class=\"media-content\" style=\"display:flex; justify-content:center;flex-direction:column;\">\n                  <div class=\"content-text\">\n                    <h3 style=\"font-size:36px\">{{ articles[index].title }}</h3>\n                    <br><hr><br>\n                    <h3 style=\"font-style:italic\">content :</h3>\n                    <br>\n                    <h3 v-html=\"articles[index].content\"></h3>\n                    <br><hr><br>\n                    <i class=\"fas fa-user-tie\"></i><a style=\"margin-right:25px\">  Owner : {{ articles[index].author.username }} </a><a style=\"margin-right:25px\"> </a><i class=\"far fa-envelope\"></i>  {{ articles[index].author.email }}\n                    <br><hr><br>\n                    <template>\n                      <b-taglist>\n                        <b-tag v-for=\"(tag, i) in articles[index].tags\" :key=\"i\" type=\"is-info\">{{tag}}</b-tag>\n                      </b-taglist>\n                    </template>\n                    <br><hr><br>\n                    <h3 style=\"font-style:italic\">galery :</h3>\n                    <br><hr><br>\n                  </div>\n\n                  <div v-if=\"articles[index].featured_image.length > 1\">\n                    <div v-for=\"(foto, i) in articles[index].featured_image\" :key=\"i\">\n                      <div class=\"card-image\">\n                        <figure class=\"image is-4by3\" >\n                          <img\n                            :src=\"foto\"\n                            alt=\"Image\"\n                          />\n                        </figure>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n      </b-modal>\n\n  </div>\n</template>\n\n<script>\nimport axios from '../../apis/server'\n\nexport default {\n  name: \"pagePersonal\",\n  data () {\n    return {\n      articles: [],\n      index: 0,\n      loadingComponent: '',\n      isCardModalActive : false\n    }\n  },\n  methods: {\n    openLoading(){\n      this.loadingComponent = this.$buefy.loading.open({\n        container: true ? null : this.$refs.element.$el\n      })\n    },\n    closeLoading(){\n      this.loadingComponent.close()\n    },\n    fetchPersonal(){\n      this.openLoading()\n      axios({\n        method: 'get',\n        url: '/article/personal',\n        headers: {\n          token: localStorage.getItem('token')\n        }\n      })\n        .then(({data}) => {\n          this.articles = data\n          closeLoading()\n        })\n        .catch(err => {\n          this.closeLoading()\n          this.$buefy.toast.open({\n            duration: 5000,\n            message: err.response.data.errors.join(', '),\n            position: 'is-top',\n            type: 'is-danger'\n          })\n        })\n    }\n  },\n  mounted(){\n    this.fetchPersonal()\n  }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"e585150c90ebc78083c637a5487108c7","cacheData":{"env":{}}}